    <script type="text/javascript" src="js/indexSignup.js"></script>
    
    <% 

    console.log('inicia javascript en html');

    let erroresEmail = [];
    let erroresPassword = [];
    let erroresFirstName = [];
    let erroresLastName = [];
    let erroresCountry = [];
    let erroresPicture = [];
    
    console.log(locals.errores);

    if(locals.errores && errores != undefined){
        locals.errores.forEach(error =>{
            let errorType = error.errorCode.substring(0,error.errorCode.indexOf('-'));
            console.log(errorType);
            switch (errorType){
                case 'email':
                    erroresEmail.push(error.message);
                    break;
                case 'pass':
                    erroresPassword.push(error.message);
                    break;
                case 'ctry':
                    erroresCountry.push(error.message);
                    break;
                case 'fname':
                    erroresFirstName.push(error.message);
                    break;
                case 'lname':
                    erroresLastName.push(error.message);
                    break;
                case 'pic':
                    erroresPicture.push(error.message);
                default:
                    console.log('no errors returned from controller');
            }; //closing swift
        }); //closing forEach
        console.log(erroresPicture);
    }; //closing if

    let usuario = ''
    let category = '';
    let adult = 'checked'

    if(locals.usuario && usuario != undefined){

        usuario = locals.usuario;

        console.log('el usuario vale: ')
        console.log(usuario)

        if(usuario.admin_category){
            if(usuario.admin_category == 1){
                category = 'checked'
            }
        }

        if(usuario.adult){
            if(adult == 0){
                adult = ''
            }
        }
    }

    %>
    
