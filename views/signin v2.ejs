            <h6>
                Inicie sesión con su cuenta <strong>CAZAKA</strong>
            </h6>
        </div>
        <div class='row justify-content-center'>
            <div class='col-12'>
                <div class='panel login-panel'>
                    <div class='panel-content'>
                        <!-- Cabecera -->
                        <p>Se está conectando a:</p>
                        <h5 class='company-name'>Tienda CAZAKA</h5>
                        <div class='signin-header'>
                            <p class='signin-instruction'>Ingrese sus credenciales</p>
                            <span class='signin-errors <%= locals.errorMessage? 'invalid' : '' %>'><%= locals.errorMessage %></span>
                        </div>
                        <!-- Formulario -->
                        <form class='form-container' action="/signin" method="post">
                            <!-- Email -->
                            <div class='error email'></div>
                            <div class='form-field'>
                                <!-- <label for='email' class='form-label email' id='username-label' name='email-label'>Correo electrónico</label> -->
                                <div class='input-label email'>
                                    <input id='username' name='email' type='text' placeholder="Direccion de correo electrónico">
                                    <label class='placeholder'>Direccion de correo electrónico</label>
                                </div>
                            </div>
                            <!-- Password -->
                            <div class='error password'></div>
                            <div class='form-field'>
                                <!-- <label for='psw' class='form-label password' id='password-label' name='password-label'>Contraseña</label> -->
                                <div class='input-label password'>
                                    <input type="password" id='password' name='psw' type='text' placeholder="Contraseña">
                                    <label class='placeholder'>Contraseña</label>
                                </div>
                            </div>
                            <!-- Boton de submit -->
                            <div class='submit-btn'>
                                <button type='submit' class='submit-button'>
                                    <span class='submit-text'>
                                        INICIAR SESION
                                    </span>
                                </button>
                            </div>
                            <!-- Keep me logged in -->
                            <div class='remember-me'>
                                <label class='remember-me-label'>
                                    <span class='checkbox-span'>
                                        <input type='checkbox' id='remember-me' name='keepMeLoggedIn' checked>
                                        <svg class='check-svg checkbox-icon' viewbox="0 0 24 24" fill='currentColor' xmlns="http://www.w3.org/2000/svg">
                                            <g>
                                                <path class="rounded lined" d="M9.7324219,16.5336914c-0.3818359,0-0.7626953-0.1425781-1.0517578-0.4277344 l-3.3730469-3.3183594c-0.1972656-0.1938477-0.1992188-0.5102539-0.0058594-0.7070312 c0.1933594-0.1982422,0.5097656-0.1982422,0.7070312-0.0058594l3.3730469,3.3183594 c0.1933594,0.1904297,0.5078125,0.1894531,0.7011719,0l7.9082031-7.7802734 c0.1972656-0.1923828,0.5136719-0.1923828,0.7070312,0.0058594c0.1933594,0.1967773,0.1914062,0.5131836-0.0058594,0.7070312 l-7.9082031,7.7802734C10.4941406,16.3911133,10.1132812,16.5336914,9.7324219,16.5336914z"></path>
                                                <path class="rounded filled" d="M9.7314453,17.0332031c-0.5078125,0-1.015625-0.1904297-1.4023438-0.5703125 l-3.3720703-3.3193359c-0.3935547-0.3867188-0.3994141-1.0200195-0.0117188-1.4135742 c0.3857422-0.3945312,1.0205078-0.3989258,1.4140625-0.0117188l3.3730469,3.3188477l7.9082031-7.7807617 c0.3925781-0.3876953,1.0273438-0.3823242,1.4140625,0.0117188c0.3876953,0.3935547,0.3818359,1.0268555-0.0117188,1.4140625 l-7.9082031,7.7807617C10.7480469,16.8427734,10.2392578,17.0332031,9.7314453,17.0332031z"></path>
                                                <path class="sharp lined" d="M9.7324219,17.140625L4.9511719 12.4375 5.6523438 11.7246094 9.7324219 15.7382812 18.3476562 7.2626953 19.0488281 7.9755859 z"></path>
                                                <path class="sharp filled" d="M9.7324219,17.8417969L4.2441406 12.4433594 5.6464844 11.0175781 9.7324219 15.0371094 18.3535156 6.5556641 19.7558594 7.9814453 z"></path>
                                            </g>
                                        </svg>
                                    </span>
                                    <span class='checkbox-span-text'>Recordarme</span>
                                </label>
                            </div>
                        </form>
                        <!-- Lost credentials -->
                        <div class='flex forgot-credential'>
                            <a class='link' id='forgot-credential'>
                                ¿Olvidó su nombre de usuario o contraseña?
                            </a>
                        </div>
                    </div>
                </div>
                <!-- Footer - Link a página de registro -->
                <div class='flex justify-content-center signup-footer'>
                    <div class='signup-option'>
                        ¿No tiene una cuenta?
                        <a class='link' href='/signup'>Regístrate</a>
                    </div>
                </div>
            </div>
        </div>
    </div>

</main>


<!-- Footer -->
<%- include('./partials/footer v2.ejs') %>

</body>
</html>
